{
	"name": "终端浏览器同步工具",
	"manifest_version": 2,
	"version": "1.0",
	"description": "终端浏览器同步工具",
	"browser_action": {
		"default_icon": "start.jpg"
	},
	"permissions": [
		"alarms",
		"webRequestBlocking",
		"webRequest",
		"http://*/*",
    	"tabs"
	],
	"icons": {
      "32": "start.jpg"
   	},
	"background": {
	  "matches": ["http://*/*"],
      "scripts": [ "jquery.min.js","config.js", "sync.js" ]
    },
    "options_page" : "popup.html"
}
